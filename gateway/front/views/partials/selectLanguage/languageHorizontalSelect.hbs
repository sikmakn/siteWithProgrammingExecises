<div class="selectContainer" id="languageSelectContainer">
    {{#if isNet}}
        <div class="isNet" id=".net"><img src="/styles/src/net-button-logo.svg">C# .NET</div>
    {{else}}
        <a href="/theme/net">C# .NET</a>
    {{/if}}

    {{#if isPython}}
        <div class="isPython"><img src="/styles/src/python-button-logo.svg">PYTHON</div>
    {{else}}
        <a href="/theme/python">PYTHON</a>
    {{/if}}

    {{#if isJs}}
        <a href="/theme/js" class="isJs"><img src="/styles/src/js-button-logo.svg">JAVASCRIPT</a>
    {{else}}
        <a href="/theme/js">JAVASCRIPT</a>
    {{/if}}

</div>